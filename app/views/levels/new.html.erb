Add new Level

<%= form_for :level, url: levels_path do |f| %>
  <br>width <%= f.text_field(:width, {:class => 'form-control'}) %> </br>
  <br>height <%= f.text_field(:height, {:class => 'form-control'}) %> </br>
  <br>start <%= f.text_field(:start, {:class => 'form-control'}) %> </br>
  <br>end <%= f.text_field(:end, {:class => 'form-control'}) %> </br>
  <br>rotation <%= f.text_field(:rotation, {:class => 'form-control'}) %> </br>
  <br>labyrinth <%= f.text_field(:labyrinth, {:class => 'form-control'}) %> </br>
  <br>blocks <%= f.text_field(:blocks, {:class => 'form-control'}) %> </br>
  <%= f.submit("Add", {:class => 'btn btn-warning'}) %>
<% end %>